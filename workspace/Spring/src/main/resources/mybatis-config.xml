<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
  <settings>
    <setting name="logImpl" value="LOG4J2"/>
    <!-- reg_id to regId -->
    <setting name="mapUnderscoreToCamelCase" value="true"/>
  </settings>
  

  <typeAliases>
    <typeAlias type="com.pcwk.ehr.code.domain.CodeVO"   alias="CodeVO"/>
    <typeAlias type="com.pcwk.ehr.user.domain.UserVO"   alias="UserVO"/>
    <typeAlias type="com.pcwk.ehr.board.domain.BoardVO" alias="BoardVO"/>
    <typeAlias type="com.pcwk.ehr.user.domain.LevelPerMemberVO" alias="LevelPerMemberVO"/>
  </typeAliases>
</configuration>    